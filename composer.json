{
    "name": "digitalroom/analytics-server",
    "description": "Analitycs Server Package",
    "type": "library",
    "version": "1.2",
    "minimum-stability": "stable",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Digitalroom\\AnalyticsServer\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Digital Room"
        }
    ],
    "require": {},
    "scripts": {
        "post-install-cmd": [
            "php artisan make:controller EventController",
            "php artisan make:migration create_event_table --create=events",
            "echo \"Route::prefix('/event')->group(function () { Route::post('/create',[\\\\App\\\\Http\\\\Controllers\\\\EventController::class,'createEvent']); });\" >> routes/web.php"
        ]
    }
}
